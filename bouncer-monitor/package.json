{
  "name": "bouncer-monitor",
  "version": "3.0.2",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development node --experimental-json-modules ./dist/indexer/cron/jobs.js",
    "prod": "NODE_ENV=production forever -c \"node --experimental-json-modules\" start ./dist/indexer/cron/jobs.js",
    "test-job": "NODE_ENV=production forever -c \"node --experimental-json-modules\" start ./dist/tests/cron/jobs.js",
    "eth-monitor": "node ./dist/monitor/ethMonitor.js",
    "transfer-monitor": "node --experimental-json-modules ./dist/monitor/transfer/monitor.js",
    "transfer-consumer": "node --experimental-json-modules ./dist/monitor/transfer/consumer.js",
    "create-project": "NODE_ENV=development node ./dist/scripts/createProject.js",
    "create-rule": "NODE_ENV=development node ./dist/scripts/createRule.js",
    "onboard-project": "NODE_ENV=development node ./dist/scripts/onBoardProject.js",
    "redis": "node ./dist/db/redis.js",
    "build": "rm -rf ./dist && tsc",
    "test": "NODE_ENV=development node --experimental-json-modules ./dist/tests/cron/jobs.js",
    "lint": "prettier-eslint --write \"**/*.ts\"",
    "update-minor": "ncu -u --target minor",
    "update": "ncu -u",
    "stop": "forever stopall"
  },
  "author": "",
  "license": "",
  "private": true,
  "dependencies": {
    "@0xkomada/bouncer-db": "^4.5.8",
    "@types/cron": "^1.7.3",
    "axios": "^0.24.0",
    "bull": "^4.2.0",
    "cron": "^1.8.2",
    "custom-env": "^2.0.1",
    "ethers": "^5.5.3",
    "express": "^4.17.2",
    "forever": "^4.0.2",
    "fs-extra": "^10.0.0",
    "helmet": "^5.0.1",
    "http-errors": "^2.0.0",
    "i": "^0.3.7",
    "joi": "^17.5.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moralis": "^0.0.183",
    "redis": "^4.0.1",
    "winston": "^3.4.0",
    "ws": "^8.4.0"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/bn.js": "^5.1.0",
    "@types/bull": "^3.15.7",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.4",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.8",
    "@types/redis": "^4.0.10",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "npm-check-updates": "^12.1.0",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "typescript": "^4.5.4"
  }
}
